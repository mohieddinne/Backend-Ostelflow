{{> header }}
<span class="preheader">{{{translate "user_signup_request_preheader"}}}</span>
<h1>{{{greetingByGender ""}}}</h1>
<h2>{{{translate "user_signup_request_preheader"}}}</h2>
<p>{{{translate "new_user_signup_request_p1"}}}</p>
<table class="body-action" align="center" width="100%" cellpadding="0" cellspacing="0" role="presentation">
  <tbody>
    <tr>
      <td align="left"><strong>{{{translate "firstName"}}}</strong></td>
      <td align="left">{{firstName}}</td>
    </tr>
    <tr>
      <td align="left"><strong>{{{translate "lastName"}}}</strong></td>
      <td align="left">{{lastName}}</td>
    </tr>
    <tr>
      <td align="left"><strong>{{{translate "email"}}}</strong></td>
      <td align="left"><a href="mailto:{{email}}">{{email}}</a></td>
    </tr>
  </tbody>
</table>
<p>{{{translate "new_user_signup_request_p2"}}}</p>
<table class="body-action" align="center" width="100%" cellpadding="0" cellspacing="0" role="presentation">
  <tr>
    <td align="center">
      <table width="100%" border="0" cellspacing="0" cellpadding="0" role="presentation">
        <tr>
          <td align="center">
            <a href="{{adminLink}}" class="f-fallback button" target="_blank">{{{translate 'admin_panel'}}}</a>
          </td>
        </tr>
      </table>
    </td>
  </tr>
</table>
{{> footer }}